      <origin xyz="0 0 -0.18" rpy="0 0 0"/>
      <mass value="2.0"/>
      <inertia ixx="0.03" ixy="0" ixz="0" iyy="0.03" iyz="0" izz="0.008"/>
    </inertial>
    <visual>
      <origin xyz="0 0 -0.18" rpy="0 0 0"/>
