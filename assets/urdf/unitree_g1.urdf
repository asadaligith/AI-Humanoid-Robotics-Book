    <limit lower="-3.14" upper="3.14" effort="50" velocity="2.0"/>
  </joint>

  <link name="right_upper_arm_link">
    <inertial>
      <origin xyz="0 0 -0.15" rpy="0 0 0"/>
      <mass value="1.5"/>
      <inertia ixx="0.02" ixy="0" ixz="0" iyy="0.02" iyz="0" izz="0.005"/>
