    </collision>
  </link>

  <joint name="left_shoulder_to_upper_arm" type="revolute">
    <parent link="left_shoulder_link"/>
    <child link="left_upper_arm_link"/>
    <origin xyz="0 0 0" rpy="0 0 0"/>
    <axis xyz="1 0 0"/>
    <limit lower="-1.57" upper="3.14" effort="50" velocity="2.0"/>
  </joint>

  <link name="left_forearm_link">
    <inertial>
      <origin xyz="0 0 -0.12" rpy="0 0 0"/>
      <mass value="1.0"/>
      <inertia ixx="0.015" ixy="0" ixz="0" iyy="0.015" iyz="0" izz="0.003"/>
    </inertial>
    <visual>
      <origin xyz="0 0 -0.12" rpy="0 0 0"/>
