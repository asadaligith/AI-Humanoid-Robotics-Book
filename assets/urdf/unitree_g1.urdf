    <origin xyz="0 0.18 0.4" rpy="0 0 0"/>
    <axis xyz="0 1 0"/>
    <limit lower="-3.14" upper="3.14" effort="50" velocity="2.0"/>
  </joint>

  <link name="left_upper_arm_link">
    <inertial>
      <origin xyz="0 0 -0.15" rpy="0 0 0"/>
